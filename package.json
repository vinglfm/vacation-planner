    "babel-core": "^6.0.20",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "bootstrap-loader": "^1.0.10",
    "bootstrap-sass": "^3.3.6",
    "css-loader": "^0.23.1",
    "cssnano": "^3.7.2",
    "enzyme": "^2.4.1",
    "eslint": "^3.1.1",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-import": "^1.11.0",
    "eslint-plugin-react": "^5.2.2",
